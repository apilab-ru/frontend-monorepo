  "scripts": {
    "filecab:popup:browser": "nx run filecab-popup:serve:browser",
    "filecab:popup:dev": "nx run filecab-popup:build:development --watch",
    "filecab:popup:build": "nx run filecab-popup:build:production",
    "filecab:cabinet:dev": "nx run filecab-cabinet:build:development --watch",
    "filecab:copy": "copyfiles -f ./apps/filecab/extension/* ./apps/filecab/dist",
    "filecab:dist": "http-server apps/filecab/dist --cors --port=3000",
    "filecab:ts:browser": "webpack --mode development --config ./apps/filecab/background/webpack.config.js --env=browser -w",
    "filecab:ts:dev": "webpack --mode development --config ./apps/filecab/background/webpack.config.js --env=development -w",
    "filecab:ts:build": "webpack --config ./apps/filecab/background/webpack.config.js --env=production --devtool=false",
    "filecab:ui-kit": "nx run filecab-ui-kit:storybook",

    "exs:copy": "copyfiles -f ./apps/extension-skeleton/extension/* ./apps/extension-skeleton/dist",
    "exs:cabinet:dev": "nx run extension-skeleton-cabinet:build:development --watch",
    "exs:popup:dev": "nx run extension-skeleton-popup:build:development --watch",
    "exs:ts:dev": "webpack --mode development --config ./apps/extension-skeleton/background/webpack.config.js --env=development -w",

    "figma:copy": "copyfiles -f ./apps/figma-extension/extension/* ./apps/figma-extension/dist",
    "figma:popup:dev": "nx run figma-extension-popup:build:development --watch",
    "figma:background:dev": "webpack --mode development --config ./apps/figma-extension/background/webpack.config.js --env=development -w",
    "figma:content:dev": "webpack --mode development --config ./apps/figma-extension/content/webpack.config.js --env=development -w",

    "start:ui-kit": "nx run ui-kit:storybook"
  },
